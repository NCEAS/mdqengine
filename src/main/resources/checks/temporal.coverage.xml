<check>
  <id>temporal.coverage</id>
  <name>Temporal coverage is present at the dataset level</name>
  <level>required</level>
  <dialect>
    <name>eml</name>
    <xpath>boolean(/*[local-name() = 'eml'])</xpath>
  </dialect>
  <selector>
    <name>temporal</name>
    <xpath>/eml/dataset/coverage/temporalCoverage</xpath>
  </selector>
  <code>
    library(metadig)
    check_presence(temporal)
  </code>
</check>
