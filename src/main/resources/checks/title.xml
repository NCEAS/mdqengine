<check>
  <id>title</id>
  <name>Title is present</name>
  <level>required</level>
  <dialect>
    <name>eml</name>
    <xpath>boolean(/*[local-name() = 'eml'])</xpath>
  </dialect>
  <selector>
    <name>title</name>
    <xpath>/eml/dataset/title</xpath>
  </selector>
  <code>
    library(metadig)
    check_presence(title)
  </code>
</check>
