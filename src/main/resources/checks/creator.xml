<?xml version="1.0" encoding="UTF-8"?>
<mdq:check xmlns:mdq="https://nceas.ucsb.edu/mdqe">
  <id>creator</id>
  <name>Creator is present</name>
  <level>required</level>
  <dialect>
    <name>eml</name>
    <xpath>boolean(/*[local-name() = 'eml'])</xpath>
  </dialect>
  <selector>
    <name>creator</name>
    <xpath>/eml/dataset/creator</xpath>
  </selector>
  <code>
    library(metadig)
    check_presence(creator)
  </code>
</mdq:check>
